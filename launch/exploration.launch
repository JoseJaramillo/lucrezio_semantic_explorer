<?xml version="1.0"?>
<launch>

  <node pkg="frontier_exploration" type="explore_client" name="explore_client" output="screen"/>

  <node pkg="frontier_exploration" type="explore_server" name="explore_server" output="screen">
    <param name="frequency" value="1.0"/>
    <param name="goal_aliasing" value="2.0"/>
    <rosparam file="$(find lucrezio_semantic_explorer)/config/costmap_common.yaml" command="load" ns="explore_costmap" />
    <rosparam file="$(find lucrezio_semantic_explorer)/config/costmap_exploration.yaml" command="load" ns="explore_costmap" />
  </node>

</launch>
